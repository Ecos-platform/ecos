<?xml version="1.0" encoding="UTF-8"?>
<ecos:ChartConfig xmlns:ecos="http://github.com/Ecos-platform/libecos/resources/schema/ChartConfig">

    <ecos:chart>
        <ecos:xyseries title="NE-plot" xLabel="East[m]" yLabel="North[m]">
            <ecos:series name="Trajectory">
                <ecos:x component="vesselModel" variable="cgShipMotion.nedDisplacement.east"/>
                <ecos:y component="vesselModel" variable="cgShipMotion.nedDisplacement.north"/>
            </ecos:series>
        </ecos:xyseries>
    </ecos:chart>

</ecos:ChartConfig>
